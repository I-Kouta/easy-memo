<%= form_with model: @prototype, local: true do |f|%>
  <div class="field">
    <%= f.label :title_history, "タイトル(必須)" %><br />
    <%= f.text_area :title_history, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :why_content, "なぜ書こうと思った？" %><br />
    <%= f.text_area :why_content, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :who_content, "誰の何を見てそう思った？" %><br />
    <%= f.text_area :who_content, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :where_content, "どこでこの話、使えそう？" %><br />
    <%= f.text_area :where_content, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :content, "メッセージ(必須)" %><br />
    <%= f.text_area :content, id:"prototype_title" %>
  </div>
<% end %>